{
  "name": "curated-tv-and-film",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://lmcjt37.github.io/curated-tv-and-film/",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "gh-pages": "^2.0.1",
    "interpolate-html-plugin": "^3.0.0",
    "intersection-observer": "^0.5.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.5",
    "reactstrap": "^6.5.0"
  },
  "scripts": {
    "start": "PORT=80 node scripts/start.js",
    "start-dev": "ENABLE_MOCK=true PORT=3000 node scripts/start.js",
    "build": "node scripts/build.js",
    "profile": "PROFILE=true node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eslint": "eslint -f node_modules/eslint/lib/formatters/html.js -o ./.reports/staticAnalysis.html ."
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/.reports/coverage",
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupTests.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/mock/browser.mock.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "\\.(css)$": "<rootDir>/node_modules/identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./.reports/junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "usePathForSuiteName": "true"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "macros"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-plugin-dual-import": "^1.2.1",
    "babel-plugin-macros": "^2.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "compression-webpack-plugin": "^1.1.11",
    "critters-webpack-plugin": "^1.1.0",
    "css-loader": "^0.28.7",
    "dotenv": "4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.4.1",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "file-loader": "^1.1.11",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "20.0.4",
    "jest-fetch-mock": "^1.5.0",
    "jest-junit": "^3.6.0",
    "mark.js": "^8.11.1",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "preload-webpack-plugin": "^3.0.0-beta.1",
    "react-dev-utils": "6.0.0-next.a671462c",
    "sinon": "^4.4.8",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "0.5.9",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.11.2",
    "webpack-manifest-plugin": "^2.0.3"
  }
}
